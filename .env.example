# Estate Hive Voice CRM Configuration
# Copy this file to .env and fill in your actual values

# OpenAI Configuration (Required for Voice CRM)
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Telegram Configuration (Required for Voice CRM)
VITE_TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Transcription Settings
# Options: openai (recommended), web-speech, auto
VITE_TRANSCRIPTION_PROVIDER=openai

# Enable/Disable Web Speech API (true/false)
VITE_USE_WEB_SPEECH_API=false

# Telegram Proxy URL (Required for Vercel deployment)
# Replace with your actual Vercel app URL
VITE_TELEGRAM_PROXY_URL=https://your-vercel-app.vercel.app/api/telegram/download

# Supabase Configuration (Required)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Alternative variable names for Vercel (optional)
# Some Vercel functions may use these instead of VITE_ prefixed ones
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
OPENAI_API_KEY=your_openai_api_key_here

# WATI WhatsApp Integration (Optional)
# Leave empty if not using WhatsApp integration
VITE_WATI_BASE_URL=
VITE_WATI_API_KEY=

# Audio Processing Settings (Optional)
VITE_AUDIO_PROCESSING_TIMEOUT=30000
VITE_MAX_RETRIES=3
VITE_RETRY_DELAY=1000