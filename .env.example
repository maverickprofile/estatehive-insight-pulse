# Estate Hive Voice CRM Configuration
# Copy this file to .env and fill in your actual values

# OpenAI Configuration (Optional - Web Speech API will be used if not provided)
VITE_OPENAI_API_KEY=

# Telegram Configuration (Optional - can be configured per bot in the UI)
VITE_TELEGRAM_BOT_TOKEN=

# Transcription Settings
# Options: web-speech, openai, auto (auto will try web-speech first, then openai)
VITE_TRANSCRIPTION_PROVIDER=auto

# Enable/Disable Web Speech API (true/false)
VITE_USE_WEB_SPEECH_API=true

# Telegram Proxy URL (Optional - for server-side file downloads)
# If not provided, will use fallback methods
VITE_TELEGRAM_PROXY_URL=

# Supabase Configuration (Required)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Audio Processing Settings (Optional)
VITE_AUDIO_PROCESSING_TIMEOUT=30000
VITE_MAX_RETRIES=3
VITE_RETRY_DELAY=1000